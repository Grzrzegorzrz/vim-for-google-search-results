{
  "name": "Vim for Google Search Results",
  "version": "1.1",
  "manifest_version": 3,

  "description": "Move between Google search results using Vim motions",

  "host_permissions": ["https://google.com/"],
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://www.google.com/*"]
    }
  ],

  "browser_specific_settings": {
    "gecko": {
      "id": "{64b97d2b-bd37-481c-b662-619e7525ca03}",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  }
}
